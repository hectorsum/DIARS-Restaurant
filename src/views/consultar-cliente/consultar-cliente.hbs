<div class="wrapper-total animate__animated animate__zoomInDown">
  <div style="background-color: transparent; width: 50%;">
    <ul class="nav nav-pills mb-3" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" href="#tab-table1" data-toggle="tab">Cliente Delivery</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#tab-table2" data-toggle="tab">Cliente Restaurant</a>
      </li>
    </ul>
    <div class="tab-content">
      <div class="tab-pane tab-pane fade show active" id="tab-table1">
        <table id="myTable1" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead class="thead-dark">
            <tr>
              <th>Name</th>
              <th>Position</th>
              <th>Office</th>
              <th>Extn.</th>
            </tr>
          </thead>
          <tbody>
          {{#each entrada}}
            <tr>
              <td scope="row">{{cod_carta}}</td>
              <td>{{nombre}}</td>
              <td>{{precio}}</td>
              <td>
                <a href="/generar-cuenta/edit/{{cod_ven}}">
                  <button type="button" class="btn btn-primary btn-sm">
                    <i class="fas fa-edit"></i>
                  </button>
                </a>
                <a target="_blank" href="/generar-cuenta/print/{{cod_ven}}">
                  <button type="button" class="btn btn-secondary btn-sm">
                    <i class="fas fa-print"></i>
                  </button>
                </a>
              </td>
            </tr>
          {{/each}}
        </tbody>
        </table>
      </div>
      <div class="tab-pane fade" id="tab-table2">
        <table id="myTable2" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead class="thead-dark">
            <tr>
              <th>Name</th>
              <th>Position</th>
              <th>Office</th>
              <th>Extn.</th>
            </tr>
          </thead>
          <tbody>
          {{#each segundo}}
            <tr>
              <td scope="row">{{cod_carta}}</td>
              <td>{{nombre}}</td>
              <td>{{precio}}</td>
              <td>
                <a href="/generar-cuenta/edit/{{cod_ven}}">
                  <button type="button" class="btn btn-primary btn-sm">
                    <i class="fas fa-edit"></i>
                  </button>
                </a>
                <a target="_blank" href="/generar-cuenta/print/{{cod_ven}}">
                  <button type="button" class="btn btn-secondary btn-sm">
                    <i class="fas fa-print"></i>
                  </button>
                </a>
              </td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>