<div class="wrapper-total animate__animated animate__zoomInDown">
  <h1 class="title-table">Consultar Cliente</h1>
  <div style="background-color: transparent; width: 70%;">
    <ul class="nav nav-pills mb-3" role="tablist">
      <li class="nav-item">
        <a class="nav-link active" href="#tab-table1" data-toggle="tab">Clientes Delivery</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#tab-table2" data-toggle="tab">Clientes Restaurante</a>
      </li>
    </ul>
    <div class="tab-content">
      <div class="tab-pane tab-pane fade show active" id="tab-table1">
        <table id="myTable1" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead>
            <tr>
              <th>#</th>
              <th>Nombres</th>
              <th>DNI</th>
              <th>Telf</th>
              <th>Email</th>
              <th>Direccion</th>
              <th>Distrito</th>
            </tr>
          </thead>
          <tbody>
          {{#each cliente}}
            <tr>
              <td scope="row">{{index @index}}</td>
              <td>{{nombre}} {{apellido}}</td>
              <td>{{dni}}</td>
              <td>{{telf}}</td>
              <td>{{email}}</td>
              <td>{{direccion}}</td>
              <td>{{distrito}}</td>
            </tr>
          {{/each}}
        </tbody>
        </table>
      </div>
      <div class="tab-pane fade" id="tab-table2">
        <table id="myTable2" class="table table-striped table-bordered" cellspacing="0" width="100%">
          <thead>
            <tr>
              <th>#</th>
              <th>Numero RUC</th>
              <th>Razon Social</th>
              <th>Direccion</th>
            </tr>
          </thead>
          <tbody>
          {{#each contribuyente}}
            <tr>
              <td scope="row">{{index @index}}</td>
              <td>{{num_ruc}}</td>
              <td>{{razon_social}}</td>
              <td>{{direccion}}</td>
            </tr>
          {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>